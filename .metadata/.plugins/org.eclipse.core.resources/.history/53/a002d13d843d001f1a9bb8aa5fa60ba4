/*     */   @EventHandler
/*     */   public void onLeftClick(NPCLeftClickEvent e) {
/* 188 */     Player p = e.getClicker();
/* 189 */     this.gamble2Utils.getGroups(p);
/* 190 */     NPC npc = e.getNPC();
/* 191 */     if (npc.getName().equalsIgnoreCase("pleb") && p.getInventory().contains(Material.NETHERITE_INGOT)) {
