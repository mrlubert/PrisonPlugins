/*    */ 
/*    */ public class Main
/*    */   extends JavaPlugin
/*    */   implements Listener
/*    */ {
/*    */   private CellCommand cellCmd;
/*    */   private KappaKCommand kappaKCmd;
/*    */   private NPCRightClick npcRClick;
/*    */   private UserBalanceUpdate balanceUpdate;
/*    */   public GambleUtils gambleUtils;
/*    */   
/*    */   public void onEnable() {
/* 52 */     this.gambleUtils = new GambleUtils();
/* 57 */     this.cellCmd = new CellCommand(this);
/* 58 */     this.kappaKCmd = new KappaKCommand(this);
/* 59 */     this.npcRClick = new NPCRightClick(this);
/* 65 */     this.balanceUpdate = new UserBalanceUpdate(this);
/* 66 */     getServer().getPluginManager().registerEvents((Listener)this.npcRClick, (Plugin)this);
/* 72 */     getServer().getPluginManager().registerEvents((Listener)this.balanceUpdate, (Plugin)this);
/* 75 */     getCommand("cell").setExecutor((CommandExecutor)this.cellCmd);
/* 76 */     getCommand("kappak").setExecutor((CommandExecutor)this.kappaKCmd);
/*    */   }
